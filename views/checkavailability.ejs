<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" type="text/css" href="Table.css">
</head>
<body>
    <table><thead><tr>
                <th>Class</th>
                <th>Fare (Rs.)</th>
                <th>Available Seats</th>
                <th>Book </th>
            </tr>
        </thead>
        <tbody id="tablebody">
            <% for(var i=0; i < jsonresponse.length; i++) { %>
                <tr>
                  <td><%= jsonresponse[i].class_name %></td>
                  <td><%= jsonresponse[i].fare %></td>
                  <td><%= jsonresponse[i].available_seats %></td>
                  <td><form method="post" action="/bookticket"><button name="<%= jsonresponse[i].class_name %>" type="submit" onclick="cancel()">Book Ticket</button></form></td>
                </tr>
                <% } %>
        </tbody>
    </table>
</body>
</html>